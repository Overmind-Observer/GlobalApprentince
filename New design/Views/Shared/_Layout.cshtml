                    </div>
                    <!-- Right Side Content / End -->
                    <div class="right-side">
                        <!--  Register Buttons -->
                        @if (ViewData["LoggeduserName"] == null)
                        {
                            <div class="header-widget hide-on-mobile">
                                <nav id="loginBtn" class="dark">
                                    <ul>
                                        <li><a href="/Account/Login">Log in</a></li>
                                        <li><a href="/Account/RegisterStudent">Sign up</a></li>
                                    </ul>
                                </nav>
                            </div>
                        }
                        else
                        {
                            var userInfo = ViewData["LoggeduserName"] as List<string>;
                            <!--  Register Buttons / End -->
                            <!-- User Menu -->
                            <div class="header-widget">
                                <!-- Messages -->
                                <div class="header-notifications user-menu">
                                    <div class="header-notifications-trigger">
                                        @if (userInfo[1] != null)
                                        {
                                            <a href="#"><div class="user-avatar status-online"><img src="~/uploads/UserImage/@userInfo[1]" alt=""></div></a>
                                        }
                                        else
                                        {
                                            <a href="#"><div class="user-avatar status-online"><img src="/content/images/user-avatar-small-01.png" alt=""></div></a>
                                        }
                                    </div>
                                    <!-- Dropdown -->
                                    <div class="header-notifications-dropdown">
                                        <!-- User Status -->
                                        <div class="user-status">
                                            <!-- User Name / Avatar -->
                                            <div class="user-details">
                                                @if (userInfo[1] != null)
                                                {
                                                    <div class="user-avatar status-online"><img src="~/uploads/UserImage/@userInfo[1]" alt=""></div>
                                                }
                                                else
                                                {
                                                    <div class="user-avatar status-online"><img src="/content/images/user-avatar-small-01.jpg" alt=""></div>
                                                }
                                                <div class="user-name">
                                                    @userInfo[0] <span></span>
                                                </div>
                                            </div>

                                            <!-- User Status Switcher -->
                                            <div class="status-switch" id="snackbar-user-status">
                                                <label class="user-online current-status">Online</label>
                                                <label class="user-invisible">Invisible</label>
                                                <!-- Status Indicator -->
                                                <span class="status-indicator" aria-hidden="true"></span>
                                            </div>
                                        </div>

                                        <ul class="user-menu-small-nav">
                                            <li><a href="dashboard.html"><i class="icon-material-outline-dashboard"></i> Dashboard</a></li>
                                            <li><a href="dashboard-settings.html"><i class="icon-material-outline-settings"></i> Settings</a></li>
                                            <li><a asp-controller="Account" asp-action="Logout"><i class="icon-material-outline-power-settings-new"></i> Logout</a></li>
                                        </ul>

                                    </div>
                                </div>

                            </div>
                            <!-- User Menu / End -->
                            <!-- Mobile Navigation Button -->

                        }
