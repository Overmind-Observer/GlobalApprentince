                                        <li><a asp-controller="Home" asp-action="JobCategories">Education & Training</a></li>
                                        <li><a asp-controller="Home" asp-action="JobCategories">Engineering</a></li>
                                        <li><a asp-controller="Home" asp-action="JobCategories">Farming, Animal & Conservation</a></li>
